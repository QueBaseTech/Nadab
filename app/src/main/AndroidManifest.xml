<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.karokojnr.nadab_hotels"><uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="Nadab Hotels"
        tools:replace="android:label"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name="com.example.karokojnr.nadab_hotels.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.karokojnr.nadab_hotels.HomeActivity"
            android:theme="@style/AppTheme" />
        <activity android:name="com.example.karokojnr.nadab_hotels.ProfileActivity" />
        <activity android:name="com.example.karokojnr.nadab_hotels.RegisterActivity" />
        <activity
            android:name="com.example.karokojnr.nadab_hotels.Items"
            android:theme="@style/AppTheme" />
        <activity android:name="com.example.karokojnr.nadab_hotels.AddMeals" />
        <activity
            android:name="com.example.karokojnr.nadab_hotels.orders.OrderList"
            android:label="@string/title_activity_order_list"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>